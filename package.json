{
  "name": "dunelin",
  "version": "0.1.0",
  "description": "CLI tool that scaffolds and manages agentic workspaces",
  "module": "src/index.ts",
  "type": "module",
  "bin": {
    "dunelin": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build --compile src/index.ts --outfile dist/dunelin",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "simple-git": "^3.27.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  }
}
